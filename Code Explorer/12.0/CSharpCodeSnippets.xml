<?xml version="1.0"?>
<!DOCTYPE MM-code-snippets>
<code-snippets language="CSharp">
	<snippet-type name="property-getter">
		<code-snippet name="Read Code" code="return &lt;!FieldName!&gt;;" state="predefined"/>
	</snippet-type>
	<snippet-type name="property-setter">
		<code-snippet name="Write Code" code="&lt;!FieldName!&gt; = value;" state="predefined"/>
		<code-snippet name="Ext. Write Code" code="if (&lt;!FieldName!&gt; != value) {\N&#x9;&lt;!FieldName!&gt; = value;\N}" state="predefined"/>
	</snippet-type>
	<snippet-type name="method-implementation">
		<code-snippet name="Default method body" code="&lt;!DefaultMethodBody!&gt;" state="predefined"/>
		<code-snippet name="Call inherited / base method" code="&lt;!CallInheritedMethod!&gt;" state="predefined"/>
	</snippet-type>
</code-snippets>
